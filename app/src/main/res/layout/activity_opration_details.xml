<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".views.activities.OprationDetails">
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:text="اكمال الحركة"
        android:layout_margin="@dimen/margin_vertical"
        android:id="@+id/complete_operation"
        />
    <include layout="@layout/include_titles_model"/>

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/items_res"
        android:layout_marginBottom="10dp">

    </androidx.recyclerview.widget.RecyclerView>
</LinearLayout>